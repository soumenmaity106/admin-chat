<%- include('../_layouts/adminheader') %>

<%- include('../_layouts/adminsidebar') %>

<!--main-container-part-->
<div id="content">
    <!--breadcrumbs-->
    <div id="content-header">

        <div id="breadcrumb"> <a href="/admin/dashboard" class="tip-bottom" data-original-title="Go to Home"><i class="icon-home"></i>
                Home</a><a href="#" class="current">Courselists </a> </div>
    </div>
    <!--End-breadcrumbs-->
    <!--Action boxes-->
    <div class="container-fluid">
        <div class="row-fluid">
            <div class="span12">
                <div class="widget-box">
                    <div class="widget-title"> <span class="icon"> <i class="icon-align-justify"></i> </span>
                        
                        <%- include('./message') %> 
                    </div>
                    <div class="widget-box">
                        <div class="widget-title"> <span class="icon"><i class="icon-th"></i></span>
                            <h5>Courselists table</h5>
                        </div>
                        <div class="widget-content nopadding">
                            <div style="overflow-x:auto;">
                                <table class="table table-bordered data-table">
                                    <thead>
                                        <tr>
                                            <th>Name</th>
                                            <th>Role</th>
                                            <th>Department</th>
                                            <th>Profile</th>
                                            <th>Profile Link</th>
                                            <th>Profileimage</th>
                                            <th>Edit</th>
                                            <th>Delete</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <% managements.forEach(function(management){ %>
                                        <tr class="gradeX">
                                            <td>
                                                <%= management.name %>
                                            </td>
                                            <td>
                                                <%= management.role %>
                                            </td>
                                            <td>
                                                <%= management.department %>
                                            </td>
                                            <td>
                                                

                                                <p style="width:80px;
                                                overflow: hidden;
                                                text-overflow: ellipsis;
                                                white-space: nowrap;">
                                                    <%= management.profile %>
                                                </p>
                                                <button title="<%= management.profile %>" type="button" class="btn btn-mini resetPrice"
                                                    data-placement='bottom'>
                                                    <i class="fa fa-info-circle" aria-hidden="true"></i>
                                                </button>
                                            </td>
                                            <td>
                                                <%= management.profilelink %>
                                            </td>
                                            <td>
                                                <%= management.profileimage %>
                                            </td>

                                            <td><a class="btn btn-info" href="/admin/management/edit-management/<%= management._id %>"><i
                                                        class="fa fa-pencil" aria-hidden="true"></i></a></td>
                                            <td><a onclick="return confirm('Are you sure you want to delete this item?');"
                                                    class="confirmdeletion btn btn-danger" href="/admin/management/delete-management/<%= management._id %>"><i
                                                        class="fa fa-trash" aria-hidden="true"></i></a></td>
                                        </tr>
                                        <% }); %>
                                    </tbody>
                                </table>
                            </div>

                        </div>
                    </div>


                </div>

            </div>
        </div>
    </div>
</div>

<%- include('../_layouts/adminfooter') %>
<script>
    $('.data-table').dataTable({
        "bJQueryUI": true,
        "sPaginationType": "full_numbers",
        "sDom": '<""l>t<"F"fp>'
    });
</script>