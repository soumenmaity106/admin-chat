<%- include('../_layouts/adminheader') %>
<%- include('../_layouts/adminsidebar') %>

<!--main-container-part-->
<div id="content">
    <!--breadcrumbs-->
    <div id="content-header">

        <div id="breadcrumb"> <a href="/admin/dashboard" class="tip-bottom" data-original-title="Go to Home"><i class="icon-home"></i>
                Home</a> <a href="/admin/coursedetail" class="tip-bottom" data-original-title="">Coursedetail</a> <a href="#" class="current">Add
                Edit Coursedetail </a> </div>
    </div>
    <!--End-breadcrumbs-->
    <!--Action boxes-->
    <div class="container-fluid">
        <div class="row-fluid">
            <div class="span12">
                <div class="widget-box">
                    <div class="widget-title"> <span class="icon"> <i class="icon-align-justify"></i> </span>
                        <h5>Edit</h5>
                    </div>
                    <div class="widget-content nopadding">
                        <form action="/admin/coursedetail/edit-coursedetail/<%= id %>" method="POST" class="form-horizontal" enctype="multipart/form-data">
                            <div class="control-group">
                                <label class="control-label">Course Id</label>
                                <div class="controls">
                                    <input type="text" class="span11" name="course_id" value="<%= course_id %>">
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">Course Name</label>
                                <div class="controls">
                                    <input type="text" class="span11" name="course_name" value="<%= course_name %>">
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">Course Fees</label>
                                <div class="controls">
                                    <input type="text" class="span11" name="course_fees" value="<%= course_fees %>">
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">Faculty Name</label>
                                <div class="controls">
                                    <input type="text" class="span11" name="faculty_name" value="<%= faculty_name %>">
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">Credits</label>
                                <div class="controls">
                                    <input type="text" class="span11" name="credits" value="<%= credits %>"> </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">Prequisites</label>
                                <div class="controls">
                                    <input type="text" class="span11" name="prequisites" value="<%= prequisites %>"> </div>
                            </div>
                    </div>
                    <div class="form-actions">
                        <button type="submit" class="btn btn-success">Update</button>
                    </div>
                    </form>
                </div>
            </div>

        </div>
    </div>
</div>
</div>
<!--end-main-container-part-->
<%- include('../_layouts/adminfooter') %>